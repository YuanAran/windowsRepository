<div class="modal fade" id="feedbackModal" tabindex="-1" role="dialog" aria-labelledby="feedbackModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header" style="border-bottom: none;">
                <h5 class="modal-title" id="feedbackModalLabel" style="color: #007bff;font-weight: bold;">意见反馈</h5>
                <button type="button" id="feedbackModalClose" class="close" data-dismiss="modal" aria-label="关闭">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label for="feedbackType">反馈类型</label>
                    <select class="form-control" id="feedbackType" required>
                        <option value="" selected disabled>请选择反馈类型</option>
                        <option value="function">功能建议（网站功能改进建议）</option>
                        <option value="music">歌曲收录（请提供歌手和歌名）</option>
                        <option value="other">其他</option>
                    </select>
                </div>
                <div class="form-group" id="link-group" style="display: none">
                    <label for="feedbackContact">错误链接</label>
                    <input type="text" class="form-control" placeholder="输入出错的网址链接" id="link" required>
                </div>
                <div class="form-group">
                    <label for="feedbackContent">反馈内容</label>
                    <textarea class="form-control" id="feedbackContent" rows="3" required
                              style="resize: none;"></textarea>
                </div>
                <div class="form-group">
                    <label for="feedbackContact">联系邮箱</label>
                    <input type="text" class="form-control" id="feedbackContact" required>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" id="resetFormBtn" class="btn btn-secondary" style="margin-right: 10px">清空表单
                </button>
                <button type="button" id="submitFormBtn" class="btn btn-primary">提交反馈</button>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    $(function () {

        let inputFeedbackType = $('#feedbackType');
        let inputFeedbackContent = $('#feedbackContent');
        let inputFeedbackContact = $('#feedbackContact');

        $('#resetFormBtn').click(function () {
            inputFeedbackContent.val('');
        });


        $('#submitFormBtn').click(function () {
            let fkjy_type = $.trim(inputFeedbackType.val());
            if (!fkjy_type) {
                alert('请先选择反馈类型');
                inputFeedbackType.focus();
                return false;
            }

            let fkjy_content = $.trim(inputFeedbackContent.val());
            if (fkjy_content.length < 1) {
                alert('请输入反馈内容~');
                inputFeedbackContent.focus();
                return false;
            }

            let fkjy_contact = $.trim(inputFeedbackContact.val());
            if (fkjy_contact.length < 1) {
                alert('请输入联系方式~');
                inputFeedbackContact.focus();
                return false;
            }
            if (fkjy_contact.indexOf('@') === -1) {
                alert('联系邮箱格式不正确~');
                inputFeedbackContact.focus();
                return false;
            }

            $.ajax({
                type: "POST",
                url: "/api/suggest",
                dataType: "json",
                data: {
                    "content": "[" + fkjy_type + "] " + fkjy_content,
                    "contact": fkjy_contact,
                },
                success: function (result) {
                    if (result.code === 1) {
                        return jAlert(result.msg, 'green');
                    }
                    return jAlert(result.msg);
                },
                error: function (e) {
                    if (e.status === 429) {
                        jAlert('提交太频繁，请稍后再试！')
                        return;
                    }
                    jAlert('【提交出错】 ' + e.responseText)
                }
            });

            $('#feedbackModal').modal('hide');
        });

        $('.get-setting-btn').click(function () {
            $.post("/api/get-common-setting",
                {
                    key: $(this).data('key'),
                },
                function (data) {
                    if (data.code === 1) {
                        jAlert(data.data.data, 'blue', true);
                    }
                });
        });

    });
</script>
