    <script type="text/javascript">
    $(function () {
        window.mp3_id = '27879';
        window.play_id = 'Q15WUHkaVFlTRVxcUnlVVQ==';
        window.mp3_url = '';
        window.mp3_url_a = '';
        window.mp3_title = '小苹果';
        window.mp3_author = '筷子兄弟';
        window.mp3_name = mp3_title + '-' + mp3_author;
        window.mp3_type = 0;
        window.mp3_cover = 'http://img3.kuwo.cn/star/albumcover/500/34/92/1889961326.jpg';
        window.error_msg = '';
        window.is_white_url = false;
        window.ut = false;
        window.mp3_extra_url = 'https://pan.xunlei.com/s/VO5wK-E0Ogeto7KEZyJpYTpfA1?pwd=ymhi#';
        window.mp3_extra_url_id = '602755';
        window.ap = new APlayer({
            container: document.getElementById('aplayer'),
            autoplay: false,
            theme: '#0e90d2',
            lrcType: 1,
            preload: 'metadata',
            // loop: 'none',
            order: 'list'
        });

        window.btn_download_mp3 = $('#btn-download-mp3');
        window.btn_download_lrc = $('#btn-download-lrc');

                    try {
            saveHistory({'title': mp3_title, 'author': mp3_author, "id": mp3_id}, 'play');
        } catch (e) {
            console.error('暂不支持保存历史记录');
        }
        
        ap.on('listswitch', function () {
            layer.tips('点击这里试听', '.aplayer-button', {
                tips: [3, '#FF5722'],
            });
        });
        
                
        ap.on('error', function (error) {
            if (error_msg.length === 0) {
                if (mp3_extra_url.length > 0) {
                    error_msg = '当前歌曲在线播放解析超时，您可以下载到本地播放~';
                } else {
                    if (is_white_url) {
                        error_msg = '当前歌曲在线播放出错，您可以下载到本地播放~';
                    } else if (ut) {
                        error_msg = '当前歌曲在线解析超时，您可以稍后刷新重试~';
                    } else {
                        error_msg = '当前歌曲在线解析出错，请尝试下载到本地播放~';
                    }
                }
            }
            ap.notice('<span class="font-weight-bolder" style="color:#FF5722;background-color:#f4f4f5"><i class="fa fa-exclamation-circle" aria-hidden="true"></i> ' + error_msg + '</span>', 3000, 1);
        });
        $('.aplayer-pic').click(function () {
            if (error_msg) {
                ap.notice('<span class="font-weight-bolder" style="color:#FF5722;background-color:#f4f4f5"><i class="fa fa-exclamation-circle" aria-hidden="true"></i> ' + error_msg + '</span>', 3000, 1);
            }
        });
        
        
                
        btn_download_mp3.click(function () {
            
            
            

            let isErr = false;
            if (is_white_url == false && error_msg.length > 0 && mp3_extra_url.length === 0) {
                isErr = true;
            }

            if (!isErr) {
                $.ajaxSettings.async = false;
                let adShow = false;

                $.ajax({
                    type: "POST",
                    url: "/api/ad-handle",
                    dataType: "json",
                    data: {
                        "mid": mp3_id,
                        "m_type": 1
                    },
                    success: function (data) {
                        if (data.code === 1) {
                            return true;
                        } else if (data.code === 2) {
                            $('#ad-modal-prompt').html('');
                            $('#ad-modal-prepend').html('');
                            $('#ad-modal-body').html(data.data.content);
                            $('#ad-modal').modal('toggle');
                            adShow = true;
                            return false;
                        } else if (data.code === 3) {
                            $('#ad-modal-prompt').html(data.data.answerPrompt);
                            $('#ad-modal-prepend').html(data.data.answerFormHtml);
                            $('#ad-modal-body').html(data.data.content);
                            $('#ad-modal').modal('toggle');
                            adShow = true;
                            return false;
                        } else if (data.code === 0) {
                            jAlert(data.msg);
                            adShow = true;
                            return false;
                        }
                    },
                    error: function (e) {
                        jAlert('请求失败，请重试！');
                        adShow = true;
                        return false;
                    },
                });

                $.ajaxSettings.async = true;
                if (adShow) return false;
            }

                                
            if (is_qq_wx_tb()) {
                return false;
            }

            if (mp3_extra_url.length !== 0) {
                                $.alert({
                    title: '<span style="color: #16b777;">请选择音质</span>',
                    content: `
<div class="d-flex justify-content-between">
<a href="${(mp3_url_a && !mp3_extra_url) ? mp3_url_a : mp3_url}" download="${mp3_name}.mp3" class="btn btn-link" target="_blank" onclick="downloadLog(mp3_id)">下载低品质MP3</a>
<a href="${mp3_extra_url}" class="btn btn-link" target="_blank" onclick="return extraUrlDo(mp3_id,mp3_extra_url)"><span class="badge badge-danger">推荐</span>下载高品质MP3</a>
</div>
`,
                    closeIcon: true,
                    typeAnimated: true,
                    backgroundDismiss: true,
                    buttons: false,
                    columnClass: 'col-md-5'
                });
                return false;
                            } else {
                downloadLog(mp3_id);
            }
        });

        //歌词按钮点击
        btn_download_lrc.click(function () {
                        let textToDownload = $('#content-lrc').text();
            let blob = new Blob([textToDownload], {type: 'text/plain'});
            let url = URL.createObjectURL(blob);
            let a = document.createElement('a');
            a.style.display = 'none'; // 隐藏a元素
            a.href = url;
            a.download = window.mp3_name + '.lrc'; // 设置下载文件的名称
            document.body.appendChild(a);
            a.click();
            document.body.removeChild(a);
            window.URL.revokeObjectURL(url);
                    });

        //获取歌曲播放地址
        $.ajax({
            type: "POST",
            url: "/api/play-url",
            dataType: "json",
            data: {id: play_id},
            success: function (result) {
                if (result.code === 1) {
                    mp3_url = result.data.url;
                    is_white_url = result.data.is_while_url;
                    ut = result.data.ut;
                } else {
                    return jAlert(result.msg);
                }
            },
            error: function (e) {
                if (mp3_type === 0) {
                    if (e.status === 429) {
                        error_msg = '请求过于频繁，请稍后再试';
                        return jAlert(error_msg);
                    }
                    error_msg = '请求失败，请刷新重试！';
                    return jAlert(error_msg);
                }
            },
            complete: function () {
                addToList();
            }
        });


        
        $('.expand').click(function () {
            if ($(this).attr('data-status') === 'up') {
                $('.content-lrc').addClass('height-auto');
                $(this).attr('data-status', 'down')
                $(this).text('收起')
            } else {
                $('.content-lrc').removeClass('height-auto');
                $(this).attr('data-status', 'up')
                $(this).text('展开')
            }
        });

        
        setTimeout('autoScroll("#download-log")', 3500);
    });


    /** 方法区 **/

    async function addToList() {
        let lrc = $('#content-lrc').text();

        if (mp3_url.indexOf('antiserver.kuwo.cn') !== -1) {
            const url = new URL(mp3_url);
            url.searchParams.set('type', 'convert_url3');
            await $.getJSON(url.href, "callback=?", function (data) {
                if (data.code === 200) {
                    mp3_url_a = data.url
                }
            });
        }

        if (mp3_extra_url.length > 0) {
            btn_download_mp3.attr('href', mp3_extra_url);
        } else if (mp3_url.length > 0) {
            btn_download_mp3.attr('href', mp3_url);
            btn_download_mp3.attr('download', mp3_name + '.mp3');
        }

        ap.list.add([{
            name: mp3_title,
            artist: mp3_author,
            url: mp3_url_a ? mp3_url_a : mp3_url,
            cover: mp3_cover,
            lrc: lrc
        }]);

        ap.template.dtime.innerHTML = '<span style="color: #999999;"></span>';
    }

    
    function autoScroll(obj) {
        $(obj).find("ul").animate({
                marginTop: "-1rem"
            },
            1000,
            function () {
                $(this).css({
                    marginTop: "0"
                }).find("li:first").appendTo(this);
                setTimeout('autoScroll("#download-log")', 3500);
            });
    }

    
    function downloadLog(id, linkType = 1, extraUrlId = 0) {
        $.post("/api/download-log", {
            music_id: id,
            link_type: linkType,
            extra_url_id: extraUrlId
        });
    }


    
    function extraUrlDo(id, url) {
        downloadLog(id, 2, mp3_extra_url_id);
                    return true;
    }

    function errFk(id) {
        let $input = null;
        jAlert(`
<ul class='list-group mt-3'>
    <li class='list-group-item'><span class='text-info'>当前歌曲：</span> <span
            style='color:#fa5661;font-weight: bolder'>${mp3_title}-${mp3_author}</span></li>
    <li class='list-group-item'>
            <textarea class='form-control border border-info' placeholder="请输入错误描述（30个字符内）" id='err-other-input'></textarea>
    </li>
</ul>
            `, 'blue', true, {
            onOpen: function () {
                $input = $('#err-other-input');
                $input.focus();
            },
            columnClass: 'col-md-5',
            buttons: {
                ok: {
                    text: '提交反馈',
                    btnClass: 'btn-blue',
                    action: function () {
                        let inputVal = $input.val().trim();
                        if (inputVal.length <= 0) {
                            alert('错误描述不能为空');
                            $input.focus();
                            return false;
                        }
                        if (inputVal.length > 30) {
                            alert('错误描述不能超过30个字');
                            $input.focus();
                            return false;
                        }
                        let btn = $(this);
                        btn.prop('disabled', true);
                        $.ajax({
                            type: "POST",
                            url: "/api/feedback",
                            dataType: "json",
                            data: {
                                "musicErrType": inputVal,
                                "musicId": id,
                            },
                            success: function (result) {
                                if (result.code === 1) {
                                    jAlert(result.msg, 'green', true, {icon: 'fa fa-check-circle', title: '提交成功'});
                                    return true;
                                } else {
                                    jAlert(result.msg);
                                    return false;
                                }
                            },
                            error: function (e) {
                                jAlert('网络请求失败，请稍后重试！');
                            },
                            complete: function () {
                                btn.prop('disabled', false);
                                return true;
                            }
                        });
                    }
                }
            }
        });
    }
</script>

    
